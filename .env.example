# ===============================
# Example .env file
# Rename to .env and fill in your values
# ===============================

# Gemini API Key for accessing the Gemini 1.5 Flash model via REST API
GEMINI_API_KEY=your_gemini_api_key_here

#Path to your Google cloud service account credentials JSON file
GOOGLE_APPLICATION_CREDENTIALS=utils/service-account.json

# Optional: Set your custom embedding model if needed (defaults to HuggingFace BGE small)
EMBEDDING_MODEL=BAAI/bge-small-en-v1.5

# Optional: Set max query rate per user (default: 5 per minute)
MAX_QUERIES=5
TIME_WINDOW=60

# Optional: Enable detailed logging
LOG_LEVEL=INFO
